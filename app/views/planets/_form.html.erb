<%= simple_form_for @planet do |f| %>
<%= f.input :name %>
<%= f.input :description %>
<%= f.input :price_per_night %>
<%= f.input :maximum_guests %>
<%= f.input :rotation_time %>
<%= f.input :revolution_time %>
<%= f.input :radius %>
<%= f.input :avg_temp %>
<%= f.input :photos, as: :file, input_html: { multiple: true } %>
<%= f.submit %>
<% end %>
